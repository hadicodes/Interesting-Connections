  <div class="container">
    <div class="jumbotron">
      <h1>Like Minded Forum</h1>
      <p>Click a topic below to connect with Like-Minded people on a topic</p>
    </div>
    <div class="col-xs-6 col-md-offset-3">
      <div class="panel panel-primary">
        <div class="panel-heading">
          <h3 class="panel-title">Forum Topics</h3>
        </div>
        <div class="panel-body">
        <ul class="messages_list">
          {{#each topic}}
      <div class="panel panel-default">
    <div class="panel-heading">
      <h3 class="panel-title .hoverable"><font color="#0F0F16"><a href="/api/posts/{{topic}}">{{topic}}</a></h3></font>
    </div>
          </div>
          {{/each}}
               </div>
      </div>
    </div>
  </div>